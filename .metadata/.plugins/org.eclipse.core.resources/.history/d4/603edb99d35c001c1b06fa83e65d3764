package com.callers;

import com.functional.Sample;

public class LamdaDemo {
   public static void main(String[] args) {
	   String x[]= {"Raj","Ravi","John"};
	 Sample s=()->{
		 System.out.println("Method m1 implementation using lambda");
	 };
	 
	 s.m1();
	// s.main(x);
	 Sample.main(x);
   }
}
