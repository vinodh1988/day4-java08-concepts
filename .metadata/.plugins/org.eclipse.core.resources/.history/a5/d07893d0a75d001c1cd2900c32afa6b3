import java.nio.file.Paths;

public class FileOperation {
     public static void main(String[] args) {
	      try {
			Stream<String> stream=Files.lines(Paths.get("D:\\File.txt"));
			
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}	
	 }
}
